# Hostinger Environment Configuration
# Upload this as .env in your Hostinger hosting directory

# Core AI Keys (required for AI features)
GOOGLE_GEMINI_API_KEY=your_actual_key_here
GOOGLE_GENAI_API_KEY=your_actual_key_here

# Public API Keys
NEXT_PUBLIC_GEMINI_API_KEY=your_actual_key_here
NEXT_PUBLIC_RAPIDAPI_KEY=your_actual_key_here
NEXT_PUBLIC_RAPID_API_KEY=your_actual_key_here

# Clerk Authentication Configuration
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_actual_key_here
CLERK_SECRET_KEY=your_actual_key_here

# Database
DATABASE_URL=your_database_url_here

# API Configuration for Hostinger
NEXT_PUBLIC_API_BASE_URL=https://your-hostinger-domain.com/api

# Environment
NODE_ENV=production
PLATFORM=hostinger